        <mxCell id="thYXyoPydOWhGMwMgY5e-119" value="4. Sécurité et validation" style="rounded=1;whiteSpace=wrap;html=1;fontSize=14;gradientColor=#E6E6E6;strokeColor=#E6E6E6;" parent="1" vertex="1">
