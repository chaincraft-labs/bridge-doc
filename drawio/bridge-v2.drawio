        <mxCell id="WKKQ7slUlsZvzBeE3rAa-12" value="persistent&lt;div&gt;storage&lt;/div&gt;" style="shape=cylinder3;whiteSpace=wrap;html=1;boundedLbl=1;backgroundOutline=1;size=15;" parent="1" vertex="1">
